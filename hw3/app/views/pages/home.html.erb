<h1> These are your users: </h1>
<% @users.each do |u| %>
	<p><%= u.username %> <%= u.email %> <%= u.age %></p>
<% end %>
<h1> These are your cats: </h1>
<% @cats.each do |c| %>
	<p><%= c.name %></p>
<% end %>

<h1> These are your unfinished tasks:</h1>

<% @unf_todos.each do |u| %>
	<p ><%= u.tasks %></p>
<% end %>

<h1> These are your finished tasks:</h1>

<% @f_todos.each do |u| %>
	<p ><%= u.tasks %></p>
<% end %>
